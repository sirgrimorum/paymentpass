{
    "name": "sirgrimorum/paymentpass",
    "type": "library",
    "description": "Payment helper for Laravel 5.6",
    "keywords": [
        "sirgrimorum",
        "PaymentPass"
    ],
    "homepage": "https://github.com/sirgrimorum/paymentpass",
    "license": "MIT",
    "authors": [{
        "name": "SirGrimorum",
        "email": "andres.espinosa@grimorum.com",
        "homepage": "https://github.com/sirgrimorum",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=7.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "Sirgrimorum\\PaymentPass\\": "src/"
        }
    },
    "autoload-dev": {

    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.5-dev"
        },
        "laravel": {
            "providers": [
                "Sirgrimorum\\PaymentPass\\PaymentPassServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}